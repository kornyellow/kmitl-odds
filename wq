print("*** New Range ***")
print("Enter Input : ", end="")
n = list(map(float, input().split(" ")))

l = []
if len(n) == 3:
    l = list(map(float, range(n[0], n[1], n[2])))
elif len(n) == 2:
    off = n[0] % int(n[0])
    l = list(map(float, range(int(n[0]), int(n[1])+1, 1)))
    l = [i+off for i in l]
else:
    l = list(map(float, range(0, int(n[0]), 1)))
r = str(l).replace("\'", "")
r = r.replace("[", "(").replace("]", ")")
print(r)
